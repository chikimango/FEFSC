<?xml version="1.0" encoding="UTF-8"?>
<project name="module_untitled4" default="compile.module.untitled4">
  <dirname property="module.untitled4.basedir" file="${ant.file.module_untitled4}"/>
  
  <property name="module.jdk.home.untitled4" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.untitled4" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.untitled4" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.untitled4" value="-encoding UTF-8 -source 1.6 -target 1.6 ${compiler.args}"/>
  
  <property name="untitled4.output.dir" value="${module.untitled4.basedir}/out/production/untitled4"/>
  <property name="untitled4.testoutput.dir" value="${module.untitled4.basedir}/out/test/untitled4"/>
  
  <path id="untitled4.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="untitled4.module.production.classpath">
    <path refid="${module.jdk.classpath.untitled4}"/>
  </path>
  
  <path id="untitled4.runtime.production.module.classpath">
    <pathelement location="${untitled4.output.dir}"/>
  </path>
  
  <path id="untitled4.module.classpath">
    <path refid="${module.jdk.classpath.untitled4}"/>
    <pathelement location="${untitled4.output.dir}"/>
  </path>
  
  <path id="untitled4.runtime.module.classpath">
    <pathelement location="${untitled4.testoutput.dir}"/>
    <pathelement location="${untitled4.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.untitled4">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.untitled4">
    <patternset refid="excluded.from.module.untitled4"/>
  </patternset>
  
  <path id="untitled4.module.sourcepath">
    <dirset dir="${module.untitled4.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.untitled4" depends="compile.module.untitled4.production,compile.module.untitled4.tests" description="Compile module untitled4"/>
  
  <target name="compile.module.untitled4.production" depends="register.custom.compilers" description="Compile module untitled4; production classes">
    <mkdir dir="${untitled4.output.dir}"/>
    <javac2 destdir="${untitled4.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.untitled4}/javac">
      <compilerarg line="${compiler.args.untitled4}"/>
      <bootclasspath refid="untitled4.module.bootclasspath"/>
      <classpath refid="untitled4.module.production.classpath"/>
      <src refid="untitled4.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.untitled4"/>
    </javac2>
    
    <copy todir="${untitled4.output.dir}">
      <fileset dir="${module.untitled4.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.untitled4.tests" depends="register.custom.compilers,compile.module.untitled4.production" description="compile module untitled4; test classes" unless="skip.tests"/>
  
  <target name="clean.module.untitled4" description="cleanup module">
    <delete dir="${untitled4.output.dir}"/>
    <delete dir="${untitled4.testoutput.dir}"/>
  </target>
</project>